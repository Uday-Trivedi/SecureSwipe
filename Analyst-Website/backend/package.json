{
  "name": "fraud-detection-backend",
  "version": "1.0.0",
  "description": "Backend API for fraud detection analyst website",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node utils/seedData.js",
